<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.patricia.fifafriends">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Fifa Friends"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CadastroActivity" />
        <activity android:name=".TabelaActivity" />
        <activity android:name=".ArtilhariaActivity" />
        <activity android:name=".RodadaAdapterActivity" />
        <activity android:name=".PlacarActivity" />
        <activity android:name=".tabela_adapterActivity" />
        <activity android:name=".Artilheiro_AdapterActivity" />
        <activity android:name=".RodadaActivity"></activity>
    </application>

</manifest>